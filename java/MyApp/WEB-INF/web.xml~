    </servlet>
 
    <servlet-mapping>
        <servlet-name>MyShowPage</servlet-name>
        <url-pattern>/my_board</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>MyShowThread</servlet-name>
        <servlet-class>MyShowThread</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>MyShowThread</servlet-name>
        <url-pattern>/show_thread</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>MyMakeNewThread</servlet-name>
        <servlet-class>MyMakeNewThread</servlet-class>
    </servlet>
 
    <servlet-mapping>
        <servlet-name>MyMakeNewThread</servlet-name>
        <url-pattern>/make_thread</url-pattern>
    </servlet-mapping>
